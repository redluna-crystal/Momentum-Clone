{
  "manifest_version": 2,

  "name": "Redluna's Momentum Clone",
  "description": "It is a personal momentum clone that has features like time, weather, todos and much more.",
  "version": "1.0",

  "chrome_url_overrides": {
    "newtab": "index.html"
  },

  "browser_action": {
    "16": "/icons/icon16.png",
    "48": "/icons/icon48.png",
    "128": "/icons/icon128.png"
  },

  "icons": {
    "16": "/icons/icon16.png",
    "48": "/icons/icon48.png",
    "128": "/icons/icon128.png"
  },

  "permissions": ["tabs", "geolocation"],

  "background": {
    "scripts": ["button-fun.js"],
    "persistent": false
  }
}
